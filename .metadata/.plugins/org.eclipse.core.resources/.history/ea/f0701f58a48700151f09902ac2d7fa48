import java.util.*;
public class Age {
	public static void main (String[] args){
		Scanner sc = new Scanner(System.in);
		int value;
		value = sc.nextInt();
		if (value>150){
			System.out.println("thats not your real age");
		}else{
			System.out.println("Congratulations on being "+ value);
			
		}
		}
	}
